    {
      "name": "Create Patient",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "if (pm.response.code === 201) {",
              "    var jsonData = pm.response.json();",
              "    pm.environment.set(\"patientId\", jsonData.id);",
              "    console.log(\"Stored patientId: \" + jsonData.id);",
              "}"
            ],
            "type": "text/javascript"
          }
        }
      ],
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n    \"name\": {\n        \"use\": \"official\",\n        \"family\": \"Doe\",\n        \"given\": [\"John\", \"A\"]\n    },\n    \"gender\": \"male\",\n    \"birthDate\": \"1985-07-20\",\n    \"active\": true\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/api/Patients",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "api",
            "Patients"
          ]
        }
      },
      "response": []
